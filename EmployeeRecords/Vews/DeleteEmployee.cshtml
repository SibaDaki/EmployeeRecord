@model EmployeeRecords.Models.EmployeeViewModel
@using EmployeeRecords.Models

@using (Html.BeginForm())
{
    @Html.Partial("_ModalHeader", new ModalHeader { Heading = "Delete Employee" })

    <div class="modal-body form-horizontal">
        Are you want to delete @Model.PersonId?
    </div>
    @Html.Partial("_ModalFooter", new ModalFooter { SubmitButtonText = "Delete" })
}

